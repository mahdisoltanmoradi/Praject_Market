
@{
    ViewData["Title"] = "ForgotPasswordSuccess";
}

<!-- Preloader-->
<div class="preloader" id="preloader">
    <div class="spinner-grow text-secondary" role="status">
        <div class="sr-only">بارگذاری...</div>
    </div>
</div>
<div class="login-wrapper d-flex align-items-center justify-content-center text-center">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-12 col-sm-9 col-md-7 col-lg-6 col-xl-5">
                <div class="px-4">
                    <!-- Success Check-->
                    <div class="success-check"><i class="lni lni-emoji-smile"></i></div>
                    <!-- Reset Password Message-->
                    <p class="text-white mt-4 mb-4">ایمیل بازیابی رمز عبور با موفقیت ارسال شد!</p>
                    <p class="text-white mt-4 mt-4">لطفا صندوق ایمیلتان را چک کنید</p>
                    <!-- Go Back Button--><a class="btn btn-warning" href="Login">ورود به سایت</a>
                </div>
            </div>
        </div>
    </div>
</div>

